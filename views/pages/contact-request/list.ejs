<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../../partials/head', {pageTitle: pageTitle}); %>
    <style>
        .table th {
            text-align: center;   
        }
    </style>
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include('../../partials/sidebar', {siteName: siteName}); %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include('../../partials/header', {siteName: siteName, userFullName: userFullName, userImage: userImage}); %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                      <!-- Page Heading -->
                      <h1 class="h3 mb-2 text-gray-800"><%-pageName%></h1>
                      <p class="mb-4">View <%-pageName%> Details</p>
  
                      <!-- DataTales Example -->
                      <div class="card shadow mb-4">
                          <div class="card-header py-3">
                              <h6 class="m-0 font-weight-bold text-primary"><%-pageName%> Details</h6>
                          </div>
                          <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Sl No</th>
                                            <th>Name</th>
                                            <th>Telephone</th>
                                            <th>Email</th>
                                            <th>Message</th>
                                            <th>Created On</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>1</td>
                                            <td>Madhumoy Ghosh</td>
                                            <td>7679802897</td>
                                            <td>madhumoy.lnsel@gmail.com</td>
                                            <td>Test Message</td>
                                            <td>22-10-2022 11:59 AM</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <hr>
                          </div>
                      </div>
                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include('../../partials/footer', {year: year, siteName: siteName}); %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    
    <%- include('../../partials/logoutModal'); %>

    <%- include('../../partials/footerJs'); %>

</body>

</html>